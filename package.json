{
  "name": "kf-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "pretest": "yarn lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "yarn format && vitest run && jest && yarn type-check",
    "test:spec": "yarn format && vitest run && yarn type-check",
    "test:watch": "vitest",
    "test:ui": "yarn format && jest --testPathPattern=\\.test\\. && yarn type-check",
    "test:coverage": "yarn format && vitest run --coverage && yarn type-check",
    "test:update": "yarn format && vitest -u && yarn type-check",
    "type-check": "tsc --noEmit --skipLibCheck && tsc --noEmit --skipLibCheck --project tsconfig.test.json"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.22",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "zod": "^4.1.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "private": true
}
