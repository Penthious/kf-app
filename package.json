{
  "name": "kf-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "lint:ci": "expo lint --max-warnings=0",
    "pretest": "yarn format && yarn lint && yarn type-check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run && jest ",
    "test:spec": "yarn format && vitest run && yarn type-check",
    "test:spec:ci": "vitest run",
    "test:watch": "vitest",
    "test:ui": "yarn format && jest --testPathPatterns=\\.test\\. && yarn type-check",
    "test:ui:ci": "jest --ci --testPathPatterns=\\.test\\.",
    "test:coverage": "yarn format && vitest run --coverage && yarn type-check",
    "test:coverage:ci": "vitest run --coverage",
    "test:update": "yarn format && vitest -u && yarn type-check",
    "type-check": "tsc --noEmit --skipLibCheck && tsc --noEmit --skipLibCheck --project tsconfig.test.json"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.8",
    "expo": "^54.0.2",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.7",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-file-system": "~19.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-print": "~15.0.7",
    "expo-router": "~6.0.1",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.9",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "^0.21.1",
    "react-native-webview": "13.16.0",
    "react-native-worklets": "^0.5.1",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.12",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-expo": "~10.0.0",
    "husky": "^9.1.7",
    "jest": "~30.1.3",
    "jest-expo": "~54.0.10",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "private": true
}
